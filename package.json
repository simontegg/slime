{
    "name": "slime",
    "displayName": "Slime",
    "description": "Slime Language",
    "version": "1.0.3",
    "publisher": "xolan",
    "repository": {
        "type": "git",
        "url": "https://github.com/xolan/slime.vscode"
    },
    "galleryBanner": {
        "color": "#45062E",
        "theme": "dark"
    },
    "icon": "images/slime-logo-2.png",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "slime",
            "aliases": ["Slime", "slime"],
            "extensions": [".slime", ".slim", ".slimleex"],
            "configuration": "./language-configuration.json"
        }],
        "snippets": [
            {
                "language": "slime",
                "path": "./snippets/snippets.json"
            }
        ],
        "grammars": [{
            "language": "slime",
            "scopeName": "text.slime",
            "path": "./syntaxes/slime.tmLanguage"
        }]
    }
}